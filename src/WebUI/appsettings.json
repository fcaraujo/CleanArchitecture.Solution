{
    "UseInMemoryDatabase": true,
    "ConnectionStrings": {
        "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=CleanArchitecture.SolutionDb;Trusted_Connection=True;MultipleActiveResultSets=true;"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "IdentityServer": {
        "Clients": {
            "CleanArchitecture.Solution.WebUI": {
                "Profile": "IdentityServerSPA"
            }
        }
    },
    "AllowedHosts": "*",
    "LocalStack": {
        "UseLocalStack": false
    },
    "AWS": {
        "Profile": "<your aws profile>",
        "Region": "eu-central-1"
    }
}
